<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
	<class name="com.entity.<ClassName></ClassName>" table="<TableName></TableName>">
		<meta attribute="extends" inherit="false">com.jy.framework.entity.Entity</meta>
		<id name="id" type="java.lang.String">
			<column name="ID" length="64" />
			<generator class="uuid.hex"></generator>
		</id>
		<content>
			<property name="<name></name>" type="<type></type>" column="<column></column>" />
		</content>
	</class>
</hibernate-mapping>
